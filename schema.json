          "type": "string",
          "const": "agent"
        },
        "agentType": {
          "type": "string",
          "const": "user"
        },
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "requirements": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "createdAt": {
          "type": "string"
        },
        "updatedAt": {
          "type": "string"
        },
        "data": {
          "type": "object",
          "properties": {
            "systemMessage": {
              "type": ["string", "null"]
            },
            "humanInputMode": {
              "$ref": "#/definitions/WaldieAgentHumanInputMode"
            },
            "maxTokens": {
              "type": ["number", "null"]
            },
            "codeExecutionConfig": {
              "$ref": "#/definitions/WaldieAgentCodeExecutionConfig"
            },
            "agentDefaultAutoReply": {
              "type": ["string", "null"]
            },
            "maxConsecutiveAutoReply": {
              "type": ["number", "null"]
            },
            "termination": {
              "$ref": "#/definitions/WaldieAgentTerminationMessageCheck"
            },
            "teachability": {
              "$ref": "#/definitions/WaldieAgentTeachability"
            },
            "modelIds": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "skills": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/WaldieAgentLinkedSkill"
              }
            },
            "nestedChats": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/WaldieAgentNestedChat"
              }
            }
          },
          "required": [
            "systemMessage",
            "humanInputMode",
            "maxTokens",
            "codeExecutionConfig",
            "agentDefaultAutoReply",
            "maxConsecutiveAutoReply",
            "termination",
            "teachability",
            "modelIds",
            "skills",
            "nestedChats"
          ],
          "additionalProperties": false
        }
      },
      "required": [
        "type",
        "agentType",
        "name",
        "description",
        "tags",
        "requirements",
        "createdAt",
        "updatedAt",
        "data"
