              "isReply": {
                "type": "boolean"
              }
            },
            "required": ["id", "isReply"],
            "additionalProperties": false
          }
        },
        "messages": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "isReply": {
                "type": "boolean"
              }
            },
            "required": ["id", "isReply"],
            "additionalProperties": false
          }
        }
      },
      "required": ["triggeredBy", "messages"],
      "additionalProperties": false
    },
    "WaldieAssistant": {
