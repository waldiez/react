        "skills": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/WaldieSkill"
          }
        },
        "chats": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/WaldieChat"
          }
        }
      },
      "required": ["nodes", "edges", "agents", "models", "skills", "chats"],
      "additionalProperties": false
