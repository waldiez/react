            "agentDefaultAutoReply",
            "maxConsecutiveAutoReply",
            "termination",
            "teachability",
            "modelIds",
            "skills",
            "nestedChats"
          ],
          "additionalProperties": false
        }
      },
      "required": [
        "type",
        "agentType",
        "name",
        "description",
        "tags",
        "requirements",
        "createdAt",
        "updatedAt",
        "data"
      ],
      "additionalProperties": false,
      "description": "Waldie Assistant Agent."
