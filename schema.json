        "nodes": {
          "type": "array",
          "items": {
            "type": "object"
          }
        },
        "edges": {
          "type": "array",
          "items": {
            "type": "object"
          }
        },
        "viewport": {
          "type": "object"
        },
        "agents": {
          "type": "object",
