    "WaldieMessageType": {
      "type": "string",
      "enum": ["string", "method", "rag_message_generator", "none"]
    },
    "WaldieEdgeLlmSummaryMethod": {
      "type": ["string", "null"],
      "enum": ["reflection_with_llm", "last_msg", null],
