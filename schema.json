            "users": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/WaldieUserProxy"
              }
            },
            "assistants": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/WaldieAssistant"
              }
            },
            "managers": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/WaldieGroupManager"
              }
            }
