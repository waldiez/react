      "additionalProperties": false,
      "description": "Waldie User Proxy Agent."
    },
    "WaldieAgentHumanInputMode": {
      "type": "string",
      "enum": ["ALWAYS", "NEVER", "TERMINATE"]
    },
    "WaldieAgentCodeExecutionConfig": {
      "anyOf": [
        {
          "$ref": "#/definitions/WaldieAgentCodeExecutionConfigDict"
        },
        {
          "type": "boolean",
          "const": false
        }
      ]
    },
    "WaldieAgentCodeExecutionConfigDict": {
      "type": "object",
      "properties": {
        "workDir": {
          "type": "string"
        },
        "useDocker": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            {
              "type": "boolean"
            }
          ]
        },
        "timeout": {
          "type": "number"
        },
        "lastNMessages": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "string",
              "const": "auto"
            }
          ]
        },
        "functions": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "additionalProperties": false
    },
    "WaldieAgentTerminationMessageCheck": {
      "type": "object",
      "properties": {
        "type": {
          "$ref": "#/definitions/WaldieAgentTypeTerminationTypeOption"
        },
        "keywords": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "criterion": {
          "anyOf": [
            {
              "$ref": "#/definitions/WaldieAgentTerminationCriterionOption"
            },
            {
              "type": "null"
            }
          ]
        },
        "methodContent": {
          "type": ["string", "null"]
        }
      },
      "required": ["type", "keywords", "criterion", "methodContent"],
      "additionalProperties": false
    },
    "WaldieAgentTypeTerminationTypeOption": {
      "type": "string",
      "enum": ["none", "keyword", "method"]
    },
    "WaldieAgentTerminationCriterionOption": {
      "type": "string",
      "enum": ["found", "ending", "exact"]
    },
    "WaldieAgentTeachability": {
      "type": "object",
      "properties": {
        "enabled": {
          "type": "boolean"
        },
        "verbosity": {
          "type": "number",
          "enum": [0, 1, 2, 3]
        },
        "resetDb": {
          "type": "boolean"
        },
        "recallThreshold": {
          "type": "number"
        },
        "maxMumRetrievals": {
          "type": "number"
        }
      },
      "required": ["enabled", "verbosity", "resetDb", "recallThreshold", "maxMumRetrievals"],
      "additionalProperties": false
    },
    "WaldieAgentLinkedSkill": {
      "type": "object",
