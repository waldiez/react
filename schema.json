            },
            "teachability": {
              "$ref": "#/definitions/WaldieAgentTeachability"
            },
            "modelIds": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "skills": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/WaldieAgentLinkedSkill"
              }
            },
            "nestedChats": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/WaldieAgentNestedChat"
              }
            }
          },
          "required": [
            "systemMessage",
            "humanInputMode",
            "maxTokens",
